package lambda;

interface Sayable{
	void say();
}

public class MethodReference {
	public static void saySomething() {
		System.out.println("Hi Riti");
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Sayable say= MethodReference::saySomething;
		say.say();

	}

}
